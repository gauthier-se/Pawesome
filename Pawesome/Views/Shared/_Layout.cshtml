    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/Pawesome.styles.css" asp-append-version="true"/>
    <script defer src="https://analytics.seyzeriat.com/script.js" data-website-id="4175f3e8-b75f-494a-a7ff-893c51519883"></script>
</head>
<body>
<partial name="_NavBar" />
    @RenderBody()
    <partial name="_Footer" />
<script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/microsoft/signalr/dist/browser/signalr.min.js"></script>
    <script src="~/js/notificationClient.js"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
